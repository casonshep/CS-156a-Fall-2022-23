    "\n",
    "data = [[1, 0], [0, 1], [0, -1], [-1, 0], [0, 2], [0, -2], [-2, 0]]\n",
    "y = [-1, -1, -1, 1, 1, 1, 1]\n",
    "\n",
    "clf = svm.SVC(kernel=\"poly\", C=inf, gamma=1, coef0=1)\n",
    "clf.fit(data, y)  \n",
    "print(clf.support_vectors_)"
